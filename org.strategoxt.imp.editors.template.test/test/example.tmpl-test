// Example "TemplateTest" program (see syntax/TemplateTest.sdf for the grammar)

module example

entity User {
  name     : String
  password : String
  homepage : URL
}

entity BlogPosting {
  poster : User
  body   : String
}

entity URL {
  location : String
}

entity Empty {

}

expression LeftAssociative {
  1 * 2 * 3
}

expression RightAssociative {
  2^2^2
}

expression Priorities {
  1 - 2 / 3
}

expression Priorities {
  1 / 2 * 3
}

expression Parentheses {
  (7 - 5)^((1 + 1) * (2 + 2))
}
