module pp/sdf2-core/Sdf2-Syntax-pp

imports
  libstratego-gpp
  runtime/tmpl/pp
  signatures/sdf2-core/Sdf2-Syntax-sig


imports
  signatures/kernel/Kernel-sig
  signatures/basic/Basic-sig
  signatures/modules/Modules-sig
  signatures/regular/Regular-sig
  signatures/lifting/Lifting-sig
  signatures/priority/Priority-sig
  signatures/characterclass/CC-sig
  signatures/sorts/Sorts-sig
  signatures/symbols/Start-Symbols-sig
  signatures/literals/Literals-sig
  signatures/labels/Labels-sig
  signatures/restrictions/Restrictions-sig
  signatures/aliases/Aliases-sig


imports
  pp/kernel/Kernel-pp
  pp/basic/Basic-pp
  pp/modules/Modules-pp
  pp/regular/Regular-pp
  pp/lifting/Lifting-pp
  pp/priority/Priority-pp
  pp/characterclass/CC-pp
  pp/sorts/Sorts-pp
  pp/symbols/Start-Symbols-pp
  pp/literals/Literals-pp
  pp/labels/Labels-pp
  pp/restrictions/Restrictions-pp
  pp/aliases/Aliases-pp


strategies
  prettyprint-example =
    prettyprint-Symbol

  prettyprint-example =
    prettyprint-Grammar

  prettyprint-Symbol :
    Start() -> [ H(
                   [SOpt(HS(), "0")]
                 , [S("<START>")]
                 )
               ]

  is-Symbol =
    ?Start()

  prettyprint-Symbol :
    FileStart() -> [ H(
                       [SOpt(HS(), "0")]
                     , [S("<Start>")]
                     )
                   ]

  is-Symbol =
    ?FileStart()

  prettyprint-Grammar :
    LexicalPriorities(t0__) -> [ H(
                                   [SOpt(HS(), "0")]
                                 , [S("lexical priorities")]
                                 )
                               , H(
                                   []
                                 , [S("")]
                                 )
                               , t0__'
                               , H(
                                   []
                                 , [S("")]
                                 )
                               ]
    with t0__' := <pp-indent(|"2")> [<pp-one-Z(prettyprint-Priorities)> t0__]

  is-Grammar =
    ?LexicalPriorities(_)

  prettyprint-Grammar :
    ContextFreePriorities(t0__) -> [ H(
                                       [SOpt(HS(), "0")]
                                     , [S("context-free priorities")]
                                     )
                                   , H(
                                       []
                                     , [S("")]
                                     )
                                   , t0__'
                                   , H(
                                       []
                                     , [S("")]
                                     )
                                   ]
    with t0__' := <pp-indent(|"2")> [<pp-one-Z(prettyprint-Priorities)> t0__]

  is-Grammar =
    ?ContextFreePriorities(_)

  prettyprint-Grammar :
    LexicalRestrictions(t0__) -> [ H(
                                     [SOpt(HS(), "0")]
                                   , [S("lexical restrictions")]
                                   )
                                 , H(
                                     []
                                   , [S("")]
                                   )
                                 , t0__'
                                 , H(
                                     []
                                   , [S("")]
                                   )
                                 ]
    with t0__' := <pp-indent(|"2")> [<pp-one-Z(prettyprint-Restrictions)> t0__]

  is-Grammar =
    ?LexicalRestrictions(_)

  prettyprint-Grammar :
    ContextFreeRestrictions(t0__) -> [ H(
                                         [SOpt(HS(), "0")]
                                       , [S("context-free restrictions")]
                                       )
                                     , H(
                                         []
                                       , [S("")]
                                       )
                                     , t0__'
                                     , H(
                                         []
                                       , [S("")]
                                       )
                                     ]
    with t0__' := <pp-indent(|"2")> [<pp-one-Z(prettyprint-Restrictions)> t0__]

  is-Grammar =
    ?ContextFreeRestrictions(_)

  is-Symbol =
    fail

  is-Grammar =
    fail