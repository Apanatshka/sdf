module example

templates

  Start.Module = <<
    module <modulename : ID>
    <Definition* ; separator="\n">
  >>

  Definition.Entity = <<
    entity <ID> {
      <Property* ; separator="\n">
    }
  >>

  Property.Property = [[
    [x : ID] : [T : Type]
  ]]

context-free syntax

  Type.Type = ID

lexical syntax

  [A-Za-z][A-Za-z0-9]* -> ID
  CONTENTCOMPLETE.CONTENTCOMPLETE = [A-Za-z0-9]* "CONTENTCOMPLETE" [0-9]+
